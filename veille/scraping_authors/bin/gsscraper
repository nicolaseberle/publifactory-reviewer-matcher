#!/Users/vincent/Documents/Publifactory/scraping_authors/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'google-scholar-scraper==0.2','console_scripts','gsscraper'
__requires__ = 'google-scholar-scraper==0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('google-scholar-scraper==0.2', 'console_scripts', 'gsscraper')()
    )
